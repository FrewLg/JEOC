{% extends 'base.html.twig' %}

{% block title %}UserFeedback{% endblock %}

{% block body %}
    <h1>UserFeedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_feedback.id }}</td>
            </tr>
            <tr>
                <th>Sent_at</th>
                <td>{{ user_feedback.sentAt ? user_feedback.sentAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>The_feedback</th>
                <td>{{ user_feedback.theFeedback }}</td>
            </tr>
            <tr>
                <th>Subject</th>
                <td>{{ user_feedback.subject }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_feedback_index') }}">back to list</a>

    <a href="{{ path('user_feedback_edit', {'id': user_feedback.id}) }}">edit</a>

    {{ include('user_feedback/_delete_form.html.twig') }}
