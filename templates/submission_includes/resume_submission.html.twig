<div class="progress">
 <div class="progress-bar progress-bar-striped 

{% if submission.progress=='5'%}  progress-bar-primary    
{% elseif submission.progress=='10' %}  progress-bar-danger
{% elseif submission.progress=='25'%}  progress-bar-warning
{% elseif submission.progress=='60'%}  progress-bar-info
{% elseif submission.progress=='70'%}  progress-bar-info
{% elseif submission.progress=='90'%}  progress-bar-notify
{% elseif submission.progress=='100'%}  progress-bar-success

		       {% endif %}"
 progress-bar-info" style="width:{{ submission.progress }}%"
 role="progressbar" aria-valuenow="50" 
 aria-valuemin="0" aria-valuemax="100">
 	</div>
 </div>
 {% if submission.complete=='1' %} <span class="label label-info"> Complete </span>
		       {% elseif submission.complete==''%} 
		       <span class="label label-danger"> Incomplete </span>
		       {% endif %}
		       <hr>
		       {% if app.user.id==submission.author.id and submission.complete=='' %}
   <a href="{{ path('submission_firststep_edit', {'id': submission.id}) }}" class='btn btn-primary btn-xs btn-block '>
                    Resume submission  </a>

{% endif %} 
